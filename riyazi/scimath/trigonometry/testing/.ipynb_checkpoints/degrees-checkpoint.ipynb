    "pi = 3.141592653589793\n",
    "def sin(X):\n",
    "    global e\n",
    "    x  = math.radians(X)\n",
    "    return((e**(x*1j)).imag)\n",
    "\n",
    "def cos(X):\n",
    "    global e\n",
    "    return((e**(X*1j)).real)"

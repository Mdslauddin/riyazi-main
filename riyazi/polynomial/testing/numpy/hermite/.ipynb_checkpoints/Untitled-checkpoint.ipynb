   "source": [
    "def hermite(x,n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    elif n==1:\n",
    "        return 2*x\n",
    "    else:\n",
    "        return 2*x*hermite(x,n-1)-2*(n-1)*hermite(x,n-2)"
   ]

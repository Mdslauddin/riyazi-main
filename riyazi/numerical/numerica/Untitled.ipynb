   "source": [
    "def solve_triangular(A, b):\n",
    "    n = len(b)\n",
    "    x = np.empty_like(b)\n",
    "    for i in range(n-1, -1, -1):\n",
    "        x[i] = b[i]\n",
    "        for j in range(n-1, i, -1):\n",
    "            x[i] -= A[i, j] * x[j]\n",
    "        x[i] /= A[i, i]\n",
    "    return x"
   ]

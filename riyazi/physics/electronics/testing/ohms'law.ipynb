    "\"\"\" ohm's law calculator\"\"\"\n",
    "\n",
    "def ohms_las(voltage=0 , current=0, resistance=0):\n",
    "    if voltage == 0: \n",
    "        return current*resistance\n",
    "    elif current ==0 :\n",
    "        return voltage/resistance\n",
    "    elif resistance == 0:\n",
    "        return voltage /current\n",
    "    else:\n",
    "        return 0"

    "def eye(ctx, n, **kwargs):\n",
    "        \"\"\"\n",
    "        Create square identity matrix n x n.\n",
    "        \"\"\"\n",
    "        A = ctx.matrix(n, **kwargs)\n",
    "        for i in xrange(n):\n",
    "            A[i,i] = 1\n",
    "        return A"

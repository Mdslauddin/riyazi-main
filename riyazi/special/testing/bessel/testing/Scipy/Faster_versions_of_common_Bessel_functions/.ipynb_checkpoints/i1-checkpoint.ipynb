   "source": [
    "import math \n",
    "def i1(x):\n",
    "    res = 0\n",
    "    for k in range(90):\n",
    "        res += (0.5*x)  *  pow((pow(x,2)/4),k) / (math.factorial(k)* math.factorial(k+1))\n",
    "    return res"
   ]

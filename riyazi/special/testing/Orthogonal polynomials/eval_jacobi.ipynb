    "from mpmath import hyp2f1\n",
    "from scipy.special import poch\n",
    "from math import gamma\n",
    "def eval_jacobi(n,alpha,beta,x):\n",
    "    return (poch(alpha+1,n)/gamma(n+1))* hyp2f1(-n, 1+alpha+beta+n, alpha+1, (1-x)/2) "
